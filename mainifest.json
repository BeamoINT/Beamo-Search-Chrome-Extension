{
  "manifest_version": 3,
  "name": "Custom Search Engine Setter",
  "version": "1.0",
  "description": "Suggests setting a custom search engine",
  "permissions": ["storage", "search"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
