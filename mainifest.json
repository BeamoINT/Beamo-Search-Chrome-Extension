{
  "manifest_version": 3,
  "name": "Beamo Search Helper",
  "version": "1.0",
  "description": "Sets Beamo Search as the user's default search engine.",
  "permissions": ["storage", "search"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
